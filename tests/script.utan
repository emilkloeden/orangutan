let in = [1, 2, 3] 
let isOdd = fn(arg) { arg % 2 == 1}
let dbl = fn(arg) { arg * 2 }
puts(dbl(4))
let out = map(dbl, in)
puts(out)
let out = filter(isOdd, in)
puts(out)
let out = filter(fn(n) { n % 2 == 0}, out)
puts(out) 
